import java.util.*;
public class Main{
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        String str = sc.nextLine();
        int n = str.length();
        char ch[] = str.toCharArray();
        int tot =0;
        for(int i =0;i<n;i++){
            if(ch[i]>='0'&&ch[i]<='9'){
                tot +=ch[i]-'0';
            }
        }
        System.out.print(tot);
       
    }
}
